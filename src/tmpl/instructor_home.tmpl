{>base/}
{<content}
<head>

</head>

<script>
  $(function() {
    pikabu = new Pikabu({
    widths: {
        left: '10%',
        right: '70%'
    }
});
  });
</script>
<div class="m-pikabu-viewport">
  <div class="m-pikabu-sidebar m-pikabu-left">
    <h2>Home</h2>
    <li><a onclick="javascript:switchAccess('gradebook','init'); return false;">Gradebook</a></li>
    <li> <a onclick="javascript:switchAccess('study','init'); return false;">Read, Study, Practice</a></li>
    <li> <a onclick="javascript:switchAccess('assignment','init'); return false;">Assignments</a><li>
  </div>
  <div class="m-pikabu-container">
    <div class="m-pikabu-overlay"></div>
    <header>
      <a class="m-pikabu-nav-toggle" data-role="left" id="left">Left Menu</a>
      <img src="https://origin.media.wiley.com/spa_assets/R16B065WP/site/wileyplus/include/images/logo.gif"
                           alt="WileyPLUS - accessible, affordable, active learning" class="header_img"/>
      <a class="m-pikabu-nav-toggle" data-role="right" id="right">Right Menu</a>
    </header>
    <div id="wrapper">
      {#content}
      {course}
      {menu}
      <div id="divider">{notification}</div>
        {sysannounce}
      {/content}
    </div>
  </div>
  <div class="m-pikabu-sidebar m-pikabu-right">
    <ul>
      <li><a title="Course Admin" onclick=nonMobile('course','init')>Course Administration</a></li>

      <li><a title="Prepare &amp; Present" onclick=nonMobile('prepare','init')>Prepare &amp; Present</a></li>
        
      <li><a id="Go_To_Student_View" href="javascript:void(0);" onclick="javascript:switchAccess('global', 'studentview'); return false;">Go To Student View</a></li>
      <li><a href="" onclick="Mobify.desktop();return false;">View Full Site</a></li>
      <li><a href="https://www.wileyplus.com/Section/id-406740.html?nocache=1">My WileyPLUS</a></li>
      <li><a href="javascript:uni_help();">Help</a></li>
      <li><a href="http://catalog.wileyplus.com/contact.html" target="_blank">Contact Us</a></li>
      <li><a href="#" onclick="uni_logout();">Log Out</a></li>
    </ul>
  </div>
</div>
<!-- Tip Content -->

<ol id="joyRideTipContent">
    <li>
        <h1>Home Page</h1>
        <p>This is the homepage navigation menu</p>
    </li>
    <li data-id="left">
        <h1>Navigation</h1>
        <p>Use this menu to navigate within main pages</p>
    </li>
    <li data-id="right">
        <h1>ETC Menu</h1>
        <p>Use this menu to navigate through extra pages</p>
    </li>
    <li data-id="viewFull" data-options="tipLocation:top">
        <h1>View Full Site</h1>
        <p>Use this link to view the desktop page you are currently on </p>
    </li>

</ol>
<!-- End Tip Content -->
<script>
  $(window).load(function() {
    $('#joyRideTipContent').joyride({
      autoStart : true,
      expose: true,
      modal: true,
        cookieMonster: true,
        cookieName: 'JoyRide'
    });
  });
</script>
<script>
$('.m-pikabu').pikabu({
    viewportSelector: '.pikabu-viewport'
});
</script>
{/content}
